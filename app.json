{
  "expo": {
    "name": "mumzworld",
    "slug": "mumzworld",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mumzworld",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hxnain.mumzworld"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/nanum-gothic/NanumGothic_400Regular.ttf",
            "node_modules/@expo-google-fonts/nanum-gothic/NanumGothic_700Bold.ttf",
            "node_modules/@expo-google-fonts/nanum-gothic/NanumGothic_800ExtraBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bb5ca168-508f-48f7-a2f3-9da848244ff1"
      }
    }
  }
}
